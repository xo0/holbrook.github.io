\f6\fs28 \cf7 DATABASE_ENGINE = 'sqlite3'\
DATABASE_NAME = '/tmp/shorturls.db'\
INSTALLED_APPS = ['shorturls']\
ROOT_URLCONF = ['shorturls.urls']\
\pard\li1540\pardirnatural
